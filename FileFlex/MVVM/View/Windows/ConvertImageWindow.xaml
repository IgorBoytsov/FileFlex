<Window x:Class="FileFlex.MVVM.View.Windows.ConvertImageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileFlex.MVVM.View.Windows" 
        xmlns:windowviewmodels="clr-namespace:FileFlex.MVVM.ViewModels.WindowViewModels" xmlns:fa="http://schemas.fontawesome.com/icons/"
        d:DataContext="{d:DesignInstance Type=windowviewmodels:ConvertImageWindowViewModel}"
        mc:Ignorable="d"
        Title="ConvertImageWindow" Height="450" Width="800">
    <Grid>
        <TextBlock FontSize="40" Text="{Binding FilesConversion.Count}"/>
    </Grid>
</Window>
